@model IEnumerable<Salon.Models.Steps>
<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">&times;</button>
            <h2>Arbeitsschritt auswählen</h2>
        </div>
        <div class="modal-body">
            @foreach (var s in Model)
            {
                <a class="list-group-item addStepEntry" href="javascript:void(0)">
                    <input type="hidden" class="inp_addStepId" value="@s.StepId" />
                    <input type="hidden" class="inp_addStepName" value="@s.Title" />
                    <label class="title">@s.Title</label>
                </a>
            }
        </div>
    </div>
</div>